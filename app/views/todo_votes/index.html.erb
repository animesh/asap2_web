<p id="notice"><%= notice %></p>

<h1>Todo Votes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @todo_votes.each do |todo_vote| %>
      <tr>
        <td><%= link_to 'Show', todo_vote %></td>
        <td><%= link_to 'Edit', edit_todo_vote_path(todo_vote) %></td>
        <td><%= link_to 'Destroy', todo_vote, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Todo Vote', new_todo_vote_path %>
