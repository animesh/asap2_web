<p id="notice"><%= notice %></p>

<h1>Hcao Terms</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hcao_terms.each do |hcao_term| %>
      <tr>
        <td><%= link_to 'Show', hcao_term %></td>
        <td><%= link_to 'Edit', edit_hcao_term_path(hcao_term) %></td>
        <td><%= link_to 'Destroy', hcao_term, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

