<p id="notice"><%= notice %></p>

<h1>Correlations</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @correlations.each do |correlation| %>
      <tr>
        <td><%= link_to 'Show', correlation %></td>
        <td><%= link_to 'Edit', edit_correlation_path(correlation) %></td>
        <td><%= link_to 'Destroy', correlation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Correlation', new_correlation_path %>
