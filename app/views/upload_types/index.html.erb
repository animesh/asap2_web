<p id="notice"><%= notice %></p>

<h1>Upload Types</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @upload_types.each do |upload_type| %>
      <tr>
        <td><%= link_to 'Show', upload_type %></td>
        <td><%= link_to 'Edit', edit_upload_type_path(upload_type) %></td>
        <td><%= link_to 'Destroy', upload_type, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Upload Type', new_upload_type_path %>
