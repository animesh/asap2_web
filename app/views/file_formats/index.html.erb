<p id="notice"><%= notice %></p>

<h1>File Formats</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @file_formats.each do |file_format| %>
      <tr>
        <td><%= link_to 'Show', file_format %></td>
        <td><%= link_to 'Edit', edit_file_format_path(file_format) %></td>
        <td><%= link_to 'Destroy', file_format, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New File Format', new_file_format_path %>
