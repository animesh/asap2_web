<p id="notice"><%= notice %></p>

<h1>Data Classes</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @data_classes.each do |data_class| %>
      <tr>
        <td><%= link_to 'Show', data_class %></td>
        <td><%= link_to 'Edit', edit_data_class_path(data_class) %></td>
        <td><%= link_to 'Destroy', data_class, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Data Class', new_data_class_path %>
