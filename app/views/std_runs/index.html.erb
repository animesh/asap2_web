<p id="notice"><%= notice %></p>

<h1>Std Runs</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @std_runs.each do |std_run| %>
      <tr>
        <td><%= link_to 'Show', std_run %></td>
        <td><%= link_to 'Edit', edit_std_run_path(std_run) %></td>
        <td><%= link_to 'Destroy', std_run, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Std Run', new_std_run_path %>
