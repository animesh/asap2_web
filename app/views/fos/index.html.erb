<p id="notice"><%= notice %></p>

<h1>Fos</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fos.each do |fo| %>
      <tr>
        <td><%= link_to 'Show', fo %></td>
        <td><%= link_to 'Edit', edit_fo_path(fo) %></td>
        <td><%= link_to 'Destroy', fo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fo', new_fo_path %>
