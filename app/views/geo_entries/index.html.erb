<p id="notice"><%= notice %></p>

<h1>Geo Entries</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @geo_entries.each do |geo_entry| %>
      <tr>
        <td><%= link_to 'Show', geo_entry %></td>
        <td><%= link_to 'Edit', edit_geo_entry_path(geo_entry) %></td>
        <td><%= link_to 'Destroy', geo_entry, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Geo Entry', new_geo_entry_path %>
