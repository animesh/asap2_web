<h4>GEO experiment <%= link_to @geo_entry.identifier, "https://www.ncbi.nlm.nih.gov/geo/query/acc.cgi?acc=" + @geo_entry.identifier %></h4>

<div class='row'>
  <div class='col-md-3'><b>Title</b></div><div class='col-md-9'><%= @geo_entry.title %></div>
</div>

<div class='row'>
  <div class='col-md-3'><b>Contacts</b></div><div class='col-md-9'><%= raw @geo_entry.contact_emails.split(";").map{|e| link_to e, "mailto:#{e}?subject=Request about dataset #{@geo_entry.identifier} [#{APP_CONFIG[:server_name]}]"}.join(" ") %></div>
</div>

<div class='row'>
  <div class='col-md-3'><b>Summary</b></div><div class='col-md-9'><%= @geo_entry.description %></div>
</div>

